<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE preferences SYSTEM 'http://java.sun.com/dtd/preferences.dtd'>

<preferences EXTERNAL_XML_VERSION="1.0">
  <root type="user">
    <map />
    <node name="geoirc">
      <map />
      <node name="personal">
      	<map>
     	  <entry key="nick1" value="Pistos|GeoIRC" />
     	  <entry key="nick2" value="Pistos[GeoIRC]" />
     	  <entry key="nick3" value="Pistos_GeoIRC" />
     	</map>
      </node>
      <node name="GUI">
        <map>
          <entry key="skin1" value="themes\aqua\gtk\gtkrc" />
          <entry key="skin2" value="themes\aqua\kde\kde.themerc" />
          <entry key="font face" value="Lucida Console" />
          <entry key="font size" value="14" />
          <entry key="start maximized" value="True" />
        </map>
      </node>
      <node name="config">
        <map>
          <entry key="default download directory" value="downloads\" />
          <entry key="confirm receive" value="true" />
        </map>
      </node>
      <node name="keyboard">
        <map>
          <entry key="|Up" value="previous_history_entry" />
          <entry key="|Down" value="next_history_entry" />
          <entry key="Alt|Left" value="previouswindow" />
          <entry key="Alt|Right" value="nextwindow" />
        </map>
      </node>
    </node>
  </root>
</preferences>

